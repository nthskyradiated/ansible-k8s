[controlplanes]
controlplane01 ansible_host=192.168.100.111
controlplane02 ansible_host=192.168.100.112

[workernodes]
node01 ansible_host=192.168.100.121
node02 ansible_host=192.168.100.122

[loadbalancers]
loadbalancer ansible_host=192.168.100.100

[all:vars]
ansible_connection=ssh
ansible_user=vagrant
ansible_ssh_private_key_file=/home/vagrant/.vagrant.d/insecure_private_key
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
